{
  "name": "electron-shadcn",
  "productName": "electron-shadcn Template",
  "version": "1.0.0",
  "description": "Electron Forge with shadcn-ui (Vite + Typescript)",
  "main": ".vite/build/main.js",
  "private": true,
  "scripts": {
    "start": "electron-forge start",
    "prestart": "npm run generate:protos",
    "dev": "source pythonvenv/bin/activate && npm run generate:protos && concurrently \"npm run dev:backend\" \"npm start\" --names \"backend,frontend\" --prefix-colors \"blue,green\"",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "premake": "npm run generate:protos",
    "publish": "electron-forge publish",
    "lint": "eslint .",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "test:all": "vitest run && playwright test",
    "build:backend": "source pythonvenv/bin/activate && cd backend && python build_server.py",
    "build:full": "npm run build:backend && npm run make",
    "dev:backend": "source pythonvenv/bin/activate && cd backend && python grpc_server.py",
    "setup:backend": "source pythonvenv/bin/activate && cd backend && pip install -r requirements.txt",
    "generate:protos": "source pythonvenv/bin/activate && node scripts/generate-protos.js",
    "generate:simple": "source pythonvenv/bin/activate && node scripts/generate-full-stack.js",
    "generate:full-stack": "source pythonvenv/bin/activate && node scripts/generate-full-stack.js",
    "test:simplified": "node scripts/test-simplified-system.js",
    "generate:protos:frontend": "protoc --plugin=protoc-gen-es=./node_modules/.bin/protoc-gen-es --es_out=src/generated --es_opt=target=ts geospatial.proto",
    "generate:protos:backend": "python -m grpc_tools.protoc --python_out=backend/generated --grpc_python_out=backend/generated --proto_path=. geospatial.proto"
  },
  "author": "ROG <luan.roger.2003@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@bufbuild/buf": "^1.55.1",
    "@bufbuild/protoc-gen-es": "^2.6.1",
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.0",
    "@electron-forge/maker-rpm": "^7.8.0",
    "@electron-forge/maker-squirrel": "^7.8.0",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron-forge/plugin-vite": "^7.8.1",
    "@electron-forge/shared-types": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.28.0",
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/react-rnd": "^8.0.0",
    "concurrently": "^8.2.2",
    "electron": "^37.2.5",
    "electron-devtools-installer": "^4.0.0",
    "electron-playwright-helpers": "^1.7.1",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "globals": "^16.2.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "tailwindcss": "^4.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0",
    "vite": "^6.3.5",
    "vitest": "^3.1.2"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.6.1",
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.7.15",
    "@icons-pack/react-simple-icons": "^12.7.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toggle": "^1.1.6",
    "@radix-ui/react-toggle-group": "^1.1.7",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/vite": "^4.1.7",
    "@tanstack/react-query": "^5.77.1",
    "@tanstack/react-router": "^1.120.20",
    "@tanstack/router-devtools": "^1.117.1",
    "@vitejs/plugin-react": "^4.6.0",
    "babel-plugin-react-compiler": "^19.0.0-beta-e993439-20250328",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "echarts": "^5.6.0",
    "electron-squirrel-startup": "^1.0.1",
    "i18next": "^25.0.1",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.5.1",
    "react-rnd": "^10.5.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "uplot": "^1.6.32",
    "zod": "^3.24.3"
  }
}
